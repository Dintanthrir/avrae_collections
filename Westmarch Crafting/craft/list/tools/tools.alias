<drac2>
using(
    craft = 'a9c460f9-1b10-488b-9bcd-32cbc9f1b17a',
    embeds = '72fea181-ba03-4cb4-8edf-1f3bc5a49578', # Owned by Lathaon#6649
)

USAGE = f"{ctx.prefix}{ctx.alias} list tools"

tools = craft.all_crafting_tools()

embed = {
    'title': "Crafting tools",
    'fields': [{
        'title': tool.name,
        'body': tool.note,
    } for tool in tools],
    'footer': USAGE
}

return embeds.get_output(embed=embed)
</drac2>